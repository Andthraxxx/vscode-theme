	"semanticHighlighting": true,
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#ffffff4f"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#e2a27c"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ed9cc2"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#81a6da"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#ba9cea"
			}
		},
		{
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#ed9cc2"
			}
		},
		{
			"scope": [
				"keyword",
			],
			"settings": {
				"foreground": "#ba9cea"
			}
		},
		{
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#ba9cea"
			}
		},
		{
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#ba9cea"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"punctuation.accessor",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#ed9cc2"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js",
				"punctuation.definition.tag",
				"punctuation.accessor.optional",
				"punctuation.separator.comma",
				"punctuation.terminator",
				"punctuation.separator",
				"punctuation.definition.interpolation",
				"punctuation.semi",
			],
			"settings": {
				"foreground": "#ffffff6f"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#74ccaa"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#ba9cea"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.namespace",
				"entity.name.type",
			],
			"settings": {
				"foreground": "#81a6da"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#ba9cea"
			}
		},
		{
			"scope": [
				"variable",
				"variable.other.constant",
			],
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#999"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e47474"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e47474"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e47474"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"fontStyle": "italic underline",
				"background": "#e47474",
				"foreground": "#24292e",
				"content": "^M"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#e47474"
			}
		},
		{
			"scope": "string variable",
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"scope": [
				"source.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#e2a27c"
			}
		},
		{
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#e2a27c"
			}
		},
		{
			"scope": "string.regexp constant.character.escape",
			"settings": {
				"foreground": "#81a6da"
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#e2a27c"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"scope": "meta.module-reference",
			"settings": {
				"foreground": "#dddddd"
			}
		},
		{
			"name": "Markdown - Link, List",
			"scope": [
				"string.other.link.title.markdown",
				"punctuation.definition.list.begin.markdown",
				"string.other.link.description",
			],
			"settings": {
				"foreground": "#ff8787"
			}
		},
		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown",
				"punctuation.definition.list_item.markdown"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Markdown - Decoration",
			"scope": [
				"punctuation.definition.link.title.begin",
				"punctuation.definition.link.title.end",
				"punctuation.definition.link.description.begin",
				"punctuation.definition.link.description.end",
				"punctuation.definition.metadata.markdown",
			],
			"settings": {
				"foreground": "#ffffff7f"
			}
		},
		{
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ba9cea"
			}
		},
		{
			"scope": "markup.quote",
			"settings": {
				"foreground": "#74ccaa",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#81a6da"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#66b395"
			}
		},
		{
			"scope": [
				"markup.underline.link"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#ffffff7f"
			}
		},
		{
			"scope": [
				"markup.strikethrough"
			],
			"settings": {
				"fontStyle": "strikethrough",
				"foreground": "#ffffff7f"
			}
		},
		{
			"name": "Markup - Raw",
			"scope": [
				"markup.inline.raw",
				"markup.raw.block",
				"markup.fenced_code.block",
			],
			"settings": {
				"foreground": "#e0c991"
			}
		},
		{
			"scope": [
				"markup.deleted",
				"punctuation.definition.deleted"
			],
			"settings": {
				"foreground": "#ff8787"
			}
		},
		{
			"scope": [
				"markup.inserted",
				"punctuation.definition.inserted"
			],
			"settings": {
				"foreground": "#74ccaa"
			}
		},
		{
			"scope": [
				"markup.changed",
				"punctuation.definition.changed"
			],
			"settings": {
				"foreground": "#81a6da"
			}
		},
		{
			"scope": [
				"markup.ignored",
				"markup.untracked"
			],
			"settings": {
				"foreground": "#999999"
			}
		},
		{
			"scope": "meta.diff.range",
			"settings": {
				"foreground": "#b392f0",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ba9cea"
			}
		},
		{
			"name": "Meta - Diff file",
			"scope": [
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"fontStyle": "",
			}
		},
		{
			"scope": "meta.separator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e2a27c"
			}
		},
		{
			"scope": "meta.output",
			"settings": {
				"foreground": "#e0c991"
			}
		},
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#ffffff7f"
			}
		},
		{
			"scope": "brackethighlighter.unmatched",
			"settings": {
				"foreground": "#ff8787"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#ed9cc2",
				"fontStyle": "underline"
			}
		}
	]
}