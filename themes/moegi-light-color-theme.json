{
	"name": "Moegi Light",
	"type": "light",
	"colors": {
		// https://code.visualstudio.com/api/references/theme-color

		// Base Colors
		"focusBorder": "#66b395",
		"foreground": "#444444",
		"disabledForeground": "#999999",
		"widget.shadow": "#00000019",
		"selection.background": "#66b3954f",
		"sash.hoverBorder": "#f18080",

		// Text colors
		"textCodeBlock.background": "#e9e9e9",
		"textLink.activeForeground": "#f18080",
		"textLink.foreground": "#f18080cc",

		// Action colors
		"toolbar.hoverBackground": "#00000010",

		// Button control
		"button.background": "#af5d5dcf",
		"button.hoverBackground": "#af5d5d",

		// Dropdown control
		"dropdown.background": "#f9f9fb",
		"dropdown.border": "#0000001a",

		// Input control
		"input.background": "#ffffff",
		"input.border": "#00000019",
		"inputOption.activeBackground": "#66b3954f",
		"inputOption.activeForeground": "#444444",

		// Scrollbar control
		"scrollbar.shadow": "#eeeeee",
		"scrollbarSlider.activeBackground": "#0000003f",
		"scrollbarSlider.background": "#0000001a",
		"scrollbarSlider.hoverBackground": "#0000002f",

		// Badge
		"badge.background": "#0000001a",
		"badge.foreground": "#333333",

		// Progress bar
		"progressBar.background": "#f18080",

		// List and trees
		"list.activeSelectionBackground": "#eeeeee",
    "list.activeSelectionForeground": "#444444",
		"list.dropBackground": "#00000008",
		"list.focusBackground": "#00000008",
		"list.focusHighlightForeground": "#66b395",
		"list.focusOutline": "#66b395",
		"list.highlightForeground": "#66b395",
		"list.hoverBackground":"#00000008",
		"list.inactiveSelectionBackground":"#eeeeee",
		"list.filterMatchBackground":"#66b39530",
		"list.filterMatchBorder": "#66b395",

		// Activity Bar
		"activityBar.background": "#f9f9fb",
		"activityBar.foreground": "#333333",
		"activityBar.border": "#0000001a",
		"activityBarBadge.foreground": "#222222",
		"activityBarBadge.background": "#96d5bd",
		"activityBar.activeBorder": "#f18080",

		// Side bar
		"sideBar.background": "#f9f9fb",
		"sideBar.border": "#0000001a",
		"sideBarSectionHeader.background":"#f9f9fa",
		"sideBarSectionHeader.border": "#0000001a",

		// Minimap
		"minimap.findMatchHighlight":"#66b3959f",
		"minimap.selectionHighlight":"#7098d49f",
		"minimap.errorHighlight": "#f18080",
		"minimap.background": "#ffffff",
		"minimapSlider.background": "#00000010",
		"minimapSlider.hoverBackground": "#00000019",
		"minimapSlider.activeBackground": "#00000029",

		// Editor Groups & Tabs
		"editorGroupHeader.tabsBackground": "#f9f9fb",
		"editorGroupHeader.tabsBorder": "#0000001a",
		"tab.activeBackground": "#ffffff",
		"tab.activeBorder": "#f18080",
		"tab.inactiveBackground": "#f9f9fb",

		// Editor colors
		"editor.background": "#ffffff",
		"editor.foreground": "#333333",
		"editorCursor.foreground": "#f18080",
		"editor.selectionBackground": "#66b3954f",
		"editor.selectionHighlightBorder": "#00000029",
		"editor.wordHighlightBackground": "#00000000",
    "editor.wordHighlightStrongBackground": "#00000000",
    "editor.wordHighlightBorder": "#66b395",
    "editor.wordHighlightStrongBorder": "#66b395",
		"editor.findMatchBackground": "#66b3954f",
		"editor.findMatchHighlightBackground": "#66b3952f",
		"editor.findMatchHighlightBorder": "#66b3955f",
		"editor.findMatchBorder": "#66b395",
		"searchEditor.findMatchBorder":"#66b395",
		"editor.hoverHighlightBackground": "#00000010",
		"editor.lineHighlightBackground": "#f6f6f6",
		"editorLink.activeForeground":"#f18080",
		"editorWhitespace.foreground":"#00000010",
		"editorIndentGuide.background": "#00000008",
		"editorIndentGuide.activeBackground": "#00000029",
		"editorInlayHint.background": "#66b3952f",
		"editorInlayHint.foreground": "#66b395",

		/// Codelens
		"editorCodeLens.foreground": "#999999",
		
		/// Bracket pair colorization
		"editorBracketHighlight.foreground1": "#91c087",
		"editorBracketHighlight.foreground2": "#66cece",
		"editorBracketHighlight.foreground3": "#7098d4",
		"editorBracketHighlight.foreground4": "#c776dc",
		"editorBracketHighlight.foreground5": "#f18080",
		"editorBracketHighlight.foreground6": "#e0ba91",

		/// Errors and warnings
		"editorError.foreground": "#f18080",
		"editorWarning.foreground": "#e7d38f",
		"editorInfo.foreground": "#7098d4",

		/// The gutter contains the glyph margins and the line numbers
		"editorGutter.modifiedBackground": "#7098d4af",
		"editorGutter.addedBackground": "#66b395af",
		"editorGutter.deletedBackground": "#f18080af",

		// Diff editor colors
		"diffEditor.insertedTextBackground": "#66b39530",
		"diffEditor.removedTextBackground": "#f1808030",
		"diffEditor.border": "#0000001a",
		"diffEditor.diagonalFill": "#00000010",
		"diffEditor.insertedLineBackground": "#66b39530",
		"diffEditor.removedLineBackground": "#f1808030",

		// Editor widget colors
		"editorWidget.background": "#fbfbfb",
		"editorSuggestWidget.background": "#fbfbfb",
		"editorSuggestWidget.selectedBackground": "#f6f6f6",
		"editorStickyScrollHover.background": "#f6f6f6",

		// Peek view colors
		"peekView.border": "#cccccc",
		"peekViewEditor.background": "#fafafc",
		"peekViewEditor.matchHighlightBackground": "#66b3953f",
		"peekViewEditor.matchHighlightBorder": "#66b395",
		"peekViewResult.matchHighlightBackground": "#66b3953f",
		"peekViewResult.selectionBackground": "#66b3953f",
		"peekViewTitle.background": "#fafafc",
    "peekViewEditorStickScroll.background": "#fafafc",

		// Merge conflicts colors
		"merge.currentHeaderBackground": "#66b3957f",
		"merge.currentContentBackground": "#66b3953f",
		"merge.incomingHeaderBackground": "#7098d47f",
		"merge.incomingContentBackground": "#7098d43f",

		// Panel colors
		"panel.background": "#f9f9fb",

		// Status bar colors
		"statusBar.background": "#ffffff",
		"statusBar.foreground": "#444444",
		"statusBar.border": "#0000001a",
		"statusBar.debuggingBackground": "#66b395",
		"statusBar.debuggingForeground": "#000000",
		"statusBar.noFolderBackground": "#222222",
		"statusBar.noFolderForeground": "#999999",
		"statusBar.focusBorder": "#00000000",
		"statusBarItem.prominentBackground": "#66b395",
		"statusBarItem.prominentForeground": "#000000",
		"statusBarItem.remoteBackground": "#66b395",
		"statusBarItem.remoteForeground": "#000000",
		"statusBarItem.errorBackground": "#f18080",
		"statusBarItem.errorForeground": "#000000",
		"statusBarItem.warningBackground": "#d8b544",
		"statusBarItem.warningForeground": "#000000",

		// Title bar colors
		"titleBar.activeBackground": "#ffffff",
		"titleBar.inactiveBackground": "#f9f9fb",
		"titleBar.border": "#0000001a",

		// Menu bar colors
		"menu.foreground": "#444444",
		"menu.background": "#f9f9fb",

		// Command Center colors
		"commandCenter.activeBackground": "#fbfbfb",
		"commandCenter.border": "#00000010",
    "commandCenter.activeBorder":"#00000019",

		// Banner colors
		"banner.foreground": "#444444",
		"banner.background": "#f9f9fb",

		// Quick picker colors
		"pickerGroup.foreground":"#66b395",
		"quickInput.background":"#fbfbfb",

		// Integrated Terminal colors
		"terminal.background": "#f9f9fb",
		"terminal.foreground": "#444444",
		"terminal.ansiBlack": "#1e1e1e",
		"terminal.ansiBlue": "#7098d4",
		"terminal.ansiBrightBlack": "#1e1e1e",
		"terminal.ansiBrightBlue": "#7098d4",
		"terminal.ansiBrightCyan": "#6ab8c0",
		"terminal.ansiBrightGreen": "#66b395",
		"terminal.ansiBrightMagenta": "#b479c3",
		"terminal.ansiBrightRed": "#e47474",
		"terminal.ansiBrightWhite": "#eeeeee",
		"terminal.ansiBrightYellow": "#e2c97e",
		"terminal.ansiCyan": "#6ab8c0",
		"terminal.ansiGreen": "#66b395",
		"terminal.ansiMagenta": "#b479c3",
		"terminal.ansiRed": "#e47474",
		"terminal.ansiWhite": "#eeeeee",
		"terminal.ansiYellow": "#e2c97e",
		"terminal.selectionBackground": "#444444",
		"terminal.tab.activeBorder": "#66b395",

		// Breadcrumbs colors
		"breadcrumb.foreground": "#999999",
	},
	"semanticHighlighting": true,
	"tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#ffffff4f"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.enummember"
      ],
      "settings": {
        "foreground": "#91c087"
      }
    },
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#e18db9"
			}
		},
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#81a6da"
      }
    },
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#bd93db"
			}
		},
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e18db9"
      }
    },
    {
      "scope": [
				"keyword",
				"keyword.operator.new",
			],
      "settings": {
        "foreground": "#bd93db"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#bd93db"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#bd93db"
      }
    },
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution",
				"punctuation.accessor",
			],
			"settings": {
				"foreground": "#e18db9"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js",
				"punctuation.definition.tag",
				"punctuation.accessor.optional",
				"punctuation.separator.comma",
				"punctuation.terminator",
				"punctuation.separator",
				"punctuation.definition.interpolation",
				"punctuation.semi",
				"keyword.operator",
			],
			"settings": {
				"foreground": "#ffffff7f"
			}
		},
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#69b397"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#bd93db"
      }
    },
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"entity.name.namespace",
				"keyword.operator.assignment",
				"entity.name.type",
			],
			"settings": {
				"foreground": "#81a6da"
			}
		},
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#bd93db"
      }
    },
    {
      "scope": [
				"variable",
				"variable.other.constant",
			],
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#999"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e47474"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e47474"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e47474"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#e47474",
        "foreground": "#24292e",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#e47474"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#91c087"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#91c087"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#81a6da"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#91c087"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
		{
			"name": "Markdown - Link, List",
			"scope": [
				"string.other.link.title.markdown",
				"punctuation.definition.list.begin.markdown",
				"string.other.link.description",
			],
			"settings": {
				"foreground": "#f18080"
			}
		},
		{
			"name": "Markdown - Plain",
			"scope": [
				"text.html.markdown",
				"punctuation.definition.list_item.markdown"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Markdown - Decoration",
			"scope": [
				"punctuation.definition.link.title.begin",
				"punctuation.definition.link.title.end",
				"punctuation.definition.link.description.begin",
				"punctuation.definition.link.description.end",
				"punctuation.definition.metadata.markdown",
			],
			"settings": {
				"foreground": "#ffffff7f"
			}
		},
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd93db"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#69b397",
				"fontStyle": "italic"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#81a6da"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#66b395"
      }
    },
    {
      "scope": [
        "markup.underline"
      ],
      "settings": {
        "fontStyle": "underline",
				"foreground": "#ffffff7f"
      }
    },
    {
      "scope": [
        "markup.strikethrough"
      ],
      "settings": {
        "fontStyle": "strikethrough",
				"foreground": "#ffffff7f"
      }
    },
		{
			"name": "Markup - Raw",
			"scope": [
				"markup.inline.raw",
				"markup.raw.block",
				"markup.fenced_code.block",
			],
			"settings": {
				"foreground": "#e0ba91"
			}
		},
    {
      "scope": [
        "markup.deleted",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#f18080"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#69b397"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#81a6da"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#b392f0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
				"fontStyle": "italic",
        "foreground": "#bd93db"
      }
    },
		{
			"name": "Meta - Diff file",
			"scope": [
				"meta.diff.header.from-file",
				"meta.diff.header.to-file"
			],
			"settings": {
				"fontStyle": "",
			}
		},
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#91c087"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#e0ba91"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#ffffff7f"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#f18080"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#e18db9",
        "fontStyle": "underline"
      }
    }
  ]
}
